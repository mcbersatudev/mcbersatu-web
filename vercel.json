{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.2",
            "excludeFiles": "{node_modules,tests}/**"
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/(.*)\\.(.+)$",
            "dest": "/public/$1.$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php",
            "headers": {
                "X-Frame-Options": "ALLOWALL",
                "Content-Security-Policy": "frame-ancestors 'self' https://www.instagram.com https://*.whatsapp.com https://vercel.app"
            }
        }
    ]
}
